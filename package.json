{
  "name": "for-jinkwon-remix",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"arc sandbox -e testing\"",
    "format": "prettier --write .",
    "format:repo": "npm run format && npm run lint:repo -- --fix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:repo": "npm run lint -- --config ./.eslintrc.repo.cjs",
    "start": "cross-env NODE_ENV=production arc sandbox",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test dev http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "icons": "npx @svgr/cli --out-dir app/assets/svgs -- app/assets/svgs"
  },
  "dependencies": {
    "@architect/architect": "^10.14.0",
    "@architect/functions": "^7.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@remix-run/architect": "^2.0.1",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "bcryptjs": "2.4.3",
    "isbot": "^3.6.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "@emailjs/browser": "^3.11.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.5",
    "@remix-run/express": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tryghost/admin-api": "^1.13.8",
    "@tryghost/content-api": "^1.11.16",
    "express": "^4.18.2",
    "react-kakao-maps-sdk": "^1.1.18",
    "react-use-draggable-scroll": "^0.4.7",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@types/react-lottie": "^1.2.7",
    "@types/tryghost__content-api": "^1.3.12",
    "@vanilla-extract/css": "^1.13.0",
    "@faker-js/faker": "^8.0.2",
    "@remix-run/eslint-config": "^2.0.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/architect__functions": "^3.13.7",
    "@types/bcryptjs": "2.4.2",
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.17.6",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.2",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "cypress": "12.17.3",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prefer-let": "^3.0.1",
    "happy-dom": "^10.10.4",
    "mock-aws-s3": "^4.0.2",
    "msw": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "prettier": "3.0.2",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.34.2"
  }
}
